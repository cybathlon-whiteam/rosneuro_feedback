<?xml version="1.0"?>
<launch>
  <node pkg="carnival" type="carnival.py" name="carnival">
    <param name="screen_width" value="480" />
    <param name="screen_height" value="240" />
    <param name="screen_scale" value="3" />
    <param name="block_interval_mean" value="10.0" />
    <param name="block_interval_stddev" value="2.0" />
    <param name="block_interval_min" value="3.0" />
    <param name="block_interval_max" value="20.0" />
    <param name="cluster_size" value="10"/>
    <param name="block_width" value="30"/>
    <param name="block_height" value="20"/>
    <param name="block_speed" value="0.5"/>
    <param name="blink_speed" value="20"/>
    <param name="blink_times" value="1"/>
  </node>
</launch>
